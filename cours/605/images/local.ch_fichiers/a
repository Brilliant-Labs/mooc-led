$.fn.tel_form_params = function() {
var params = {}
$(this).find('input,select').each(function() {
var name = $(this).attr('name');
var val  = $(this).val();
var type = $(this).attr('type');
if (type == 'checkbox')
val = $(this).is(':checked') + 0;
if (name && val && type != 'submit')
params[name] = val;
});
if (params.privat && params.firma) {
delete params.privat;
delete params.firma;
}
return params;
}
function tel_showdupes(link, telid) {
var target = $(link).parents('article.tel-resultentry');
var spinner = $("<img>", {src:"//lib.search.ch/images/spinner-e6e6e6.gif", style:"height:16px;width:16px"});
$(link).parent().append(spinner);
$(link).hide();
$.getJSON('/api/', {action:'showdupes', id:telid, lang:env.lang}, function(data) {
spinner.hide();
if (data.html)
target.append(data.html);
});
}
