function U(path, params)
{
return params && !$.isEmptyObject(params) ? path + (path.match(/\?/) ? "&" : "?") + $.param(params) : path;
}
function LU(path, params)
{
var locparts = document.location.pathname.match(/(\.\w\w)\.html$/);
var myhostparts = document.location.hostname.match('^[-a-z]+\.(.*)');
path = path.replace(/^(\/\/\w+)\//, "$1." + (myhostparts? myhostparts[1] : "") + "/");
return U(locparts ? path.replace(/\/($|\?)/, "/index.html" + "$1").replace(/(\.html|\.gif|\.jpg|\.png|\.json|\.php)($|\?)/, locparts[1] + "$1$2", path).replace(/index.html($|\?)/, "$1") : path, params);
}
function Q(value)
{
return typeof value == "undefined" ? "" : value.toString().replace(/&/g, '&amp;').replace(/\"/g, '&quot;').replace(/[<]/g, '&lt;').replace(/>/g, '&gt;');
}
$(document).on('click', '.sl-collapsible h2', function() {
var elem = $(this).closest('.sl-collapsible');
elem.find(".sl-card-body").slideToggle(150, function() { elem.toggleClass('sl-collapsed').trigger('sl_collapse', { id:elem.attr('id'), collapsed:elem.hasClass('sl-collapsed') }); });
});
$(document).on('click', '.sl-toggle-option', function() {
var
currentopt = $(this).siblings('.sl-is-on'),
newopt = $(this);
if (!currentopt.length && ($(this).siblings('.sl-toggle-option').length == 1)) {
currentopt = $(this);
newopt = $(this).siblings('.sl-toggle-option');
}
if (currentopt.length) {
currentopt.toggleClass('sl-is-on');
newopt.toggleClass('sl-is-on');
$(this).siblings('.sl-toggle-thumb').css('left', newopt.css('left'));
$(this).siblings('input').val(newopt.data('value')).trigger('change');
}
});
