sl_add_via(form);
e.preventDefault();
});
$(".sl-form-row-action.sl-dirswitch").click(function (e) {
$('.ui-autocomplete-input').autocomplete('close');
var form = $(this).parents('form');
var from = form.find('.sl-route-from');
var to = form.find('.sl-route-to');
var oldfrom = from.val();
from.val(to.val()).trigger('change');
to.val(oldfrom).trigger('change');
var via = form.find('.sl-route-via:visible').map(function(){return $(this).val();}).get().reverse();
form.find('.sl-route-via:visible').val(function(i){return via[i];}).trigger('change');
e.preventDefault();
});
});
$(function() {
if ((String(navigator.platform).toLowerCase().indexOf('win') >= 0) && document.getElementsByTagName && (flashs = document.getElementsByTagName(document.all && !(navigator.userAgent && navigator.userAgent.indexOf('Opera') >=0) ? 'object' : 'embed')))
{
for (var i = 0; i < flashs.length; i++)
{
var flashcontainer = flashs[i].tagName.toLowerCase() == "object" ? flashs[i].parentNode : flashs[i].parentNode.parentNode;
flashs[i].style.zindex = 0;
var flashparent = flashs[i].parentNode;
if (flashs[i].tagName && flashs[i].tagName.toLowerCase() == 'embed')
{
var newembed = flashparent.innerHTML.replace(/<embed/i, '<embed wmode="opaque"');
flashparent.innerHTML = newembed;
}
else if (flashs[i].outerHTML)
{
var parts = flashs[i].outerHTML.split(/(<)/);
flashparent.innerHTML = "<" + parts[1] + "<param name='wmode' value='opaque'>" + flashs[i].innerHTML + "<" + parts[parts.length - 1];
}
}
}
});
$(function() {
function resize_tribune()
{
$(".sl-tribune-ad").each(function() {
$(this)	.height($(window).height() - $(this).offset().top - 40)
.width($(window).width() - $(this).offset().left - 20);
});
}
var content_width = 1236 + 20 + 5;
var ad = [];
if ($(window).width() > content_width + 300)
ad = $("#sl-tribune-big");
else if ($(window).width() > content_width + 160)
ad = $("#sl-tribune-small");
if (ad.length)
{
ad.html(ad.html().replace(/<!--/, "").replace(/-->/, ""));
resize_tribune();
$(window).on('resize', resize_tribune);
}
});
